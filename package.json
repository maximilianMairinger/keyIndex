{
  "name": "key-index",
  "version": "1.4.11",
  "description": "Key based index store.",
  "main": "app/dist/esm/keyIndex.js",
  "types": "app/dist/esm/keyIndex.d.ts",
  "exports": {
    "node": {
      "import": "./app/dist/esm/keyIndex.mjs",
      "require": "./app/dist/cjs/keyIndex.js"
    },
    "default": "./app/dist/esm/keyIndex.js"
  },
  "scripts": {
    "build": "npm run buildCJS && npm run buildESM",
    "buildCJS": "npx del-cli app/dist/cjs && tsc -p ./tsconfig.prod.cjs.json",
    "devCJS": "npx del-cli app/dist/cjs && tsc -p ./tsconfig.prod.cjs.json -w",
    "buildESM": "npx del-cli app/dist/esm && tsc -p ./tsconfig.prod.esm.json && npx ncp app/dist/esm app/dist/esm2; npx renamer --find /\\.js$/i --replace .mjs app/dist/esm2/**; npx ncp app/dist/esm2 app/dist/esm; npx del-cli app/dist/esm2",
    "dev": "npm run devNode",
    "devWeb": "rollup --config rollup.web.dev.config.js -w",
    "devNode": "rollup --config rollup.node.dev.config.js -w",
    "deploy": "npm run build && npm publish",
    "repl": "node --trace-warnings ./repl/dist/imageWeb-repl.js",
    "start": "npm run repl",
    "buildTest": "rollup --config rollup.node.test.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maximilianMairinger/keyIndex.git"
  },
  "keywords": [
    "key",
    "index"
  ],
  "author": "Maximilian Mairinger",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maximilianMairinger/keyIndex/issues"
  },
  "homepage": "https://github.com/maximilianMairinger/keyIndex#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-typescript": "5.0.0",
    "@types/node": "^13.13.4",
    "del-cli": "^5.0.0",
    "ncp": "^2.0.0",
    "renamer": "^4.0.0",
    "rollup": "^2.18.1",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5",
    "webpack-merge": "^5.0.9"
  },
  "dependencies": {
    "get-class-function-names": "^1.1.1"
  }
}
